{
	"$schema": "vscode://schemas/color-theme",
	"name": "VSXcode Default(light)",
	"colors": {
		"editor.background": "#FFFFFF",
		"editor.foreground": "#000000",
		"editor.lineHighlightBackground": "#edf5fe",
		"editor.selectionBackground": "#B6D7FC",
		"editor.selectionHighlightBackground": "#B6D7FC55",
		"editor.wordHighlightBackground": "#B6D7FC0F",
		"editor.inactiveSelectionBackground": "#D2D2D2",
		"editorLineNumber.foreground": "#BCBCBC",
		"editorLineNumber.activeForeground": "#000000",
		"tab.activeBackground": "#D4E7FD",
		"tab.activeBorder": "#D3D3D3",
		"tab.inactiveBackground": "#FFFFFF",
		"errorForeground": "#F9C3C1",
		"editorWarning.foreground": "#000000",
		"editor.stackFrameHighlightBackground": "#E2F3E1",
		"debugIcon.breakpointForeground": "#177BF5",
		"debugIcon.breakpointCurrentStackframeForeground": "#177BF5",
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#5D6C79"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#1C00CF"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#815F03"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#815F03"
			}
		},
		{
			"scope": [
				"variable.other.macro.argument.cpp",
			],
			"settings": {
				"foreground": "#815F03"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor",
				"keyword.control.directive.pragma",
				"keyword.control.directive.include.objc",
				"keyword.control.directive.include.objcpp",
				"keyword.control.directive.import.objc",
				"keyword.control.directive.import.objcpp",
				"keyword.control.preprocessor.conditional.swift"
			],
			"settings": {
				"foreground": "#643820",
				"fontStyle": "",
			}
		},
		{
			"name": "Pragma Mark Name",
			"scope": [
				"entity.name.tag.pragma-mark"
			],
			"settings": {
				"foreground": "#4A5560",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#C41A16"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#1C00CF"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#632301"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.type.objc",
				"storage.type.objcpp",
				"storage.type.cpp",
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#C41A16"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#C41A16"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#C41A16"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"keyword",
				"constant.language"
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"keyword.control",
				"storage.type.built-in.primitive.objcpp",
			],
			"settings": {
				"foreground": "#9B2393",
				"fontStyle": "bold",
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#2b2b2b"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"scope": [
				"entity.other.inherited-class.objc",
				"meta.interface-or-protocol.objc",
			],
			"settings": {
				"foreground": "#3900A0",
			}
		},
		{
			"scope": [
				"meta.argument-type.objc",
				"meta.return-type.objc",
			],
			"settings": {
				"foreground": "#1C00CF",
			}
		},
		{
			"scope": [
				"variable.parameter.function.objc",
			],
			"settings": {
				"foreground": "#000000",
			}
		},
		{
			"scope": "entity.name.type.objc",
			"settings": {
				"foreground": "#0B4F79",
			}
		},
		{
			"scope": [
				"punctuation.definition.type.begin.objc",
				"punctuation.definition.type.end.objc",
				"punctuation.section.scope.begin.objc",
				"punctuation.section.scope.end.objc",
				"punctuation.definition.entity.other.inherited-class.objc",
				"punctuation.terminator.statement.objc",
			],
			"settings": {
				"foreground": "#000000",
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#1C00CF"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"variable.other.member.objc",
				"variable.other.member.objcpp",
				"variable.other.object.property",
				"entity.name.operator.custom-literal",
			],
			"settings": {
				"foreground": "#0F68A0"
			}
		},
		{
			"scope": "support.function.any-method.objc",
			"settings": {
				"foreground": "#6C36A9"
			}
		},
		{
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.control",
				"source.cpp keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.operator",
				"entity.name.operator",
				"storage.type.objc"
			],
			"settings": {
				"foreground": "#9B2393"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"entity.other.inherited-class",
			],
			"settings": {
				"foreground": "#1C464A"
			}
		},
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#9B2393",
		"stringLiteral": "#C41A16",
		"customLiteral": "#1C00CF",
		"numberLiteral": "#1C00CF",
		"property": "#326D74",
		"class": "#1C464A",
		"interface": "#1C464A",
		"struct": "#1C464A",
		"typeParameter": "#1C464A",
		"class.declaration": "#0B4F79",
		"interface.declaration": "#0B4F79",
		"struct.declaration": "#0B4F79",
		"function": "#326D74",
		"function.declaration": "#0F68A0",
		"enumMember": "#326D74",
		"enumMember.declaration": "#0F68A0",
		"method.declaration": "#326D74",
		"property.declaration": "#0F68A0",
		"identifier:swift": "#0F68A0",
		"class.defaultLibrary": "#3900A0",
		"interface.defaultLibrary": "#3900A0",
		"struct.defaultLibrary": "#3900A0",
		"type.defaultLibrary": "#9B2393",
		"typeParameter.defaultLibrary": "#3900A0",
		"function.defaultLibrary": "#6C36A9",
		"enumMember.defaultLibrary": "#6C36A9",
		"method.defaultLibrary": "#6C36A9",
		"property.defaultLibrary": "#6C36A9",
		"macro": "#643820",
		"keyword": "#9B2393",
	},
}